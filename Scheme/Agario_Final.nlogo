breed [mains main]
breed [computers computer]
turtles-own [points lmao]
to worldsetup
  ca
  resize-world 0 100 0 44
  set-patch-size 9.9
  end
to mainsetup
  create-mains 1 [ set shape "circle"
    set color red
    setxy mouse-xcor mouse-ycor
    set label "YOU"
    set label-color white
    set points 1
    set lmao 1]
end
to computersetup
  create-computers num_computers [set shape "circle"
    set size random 3
    set label "computer"
    set points 1
    setxy random -60 random -60
    set lmao 2]
end
to patchsetup
  ask patches [set pcolor black
    if random 100 > patchdensity [set pcolor random 140]
  if random 100 > reddensity [set pcolor red]]
end

to fullsetup
  worldsetup
  mainsetup
  computersetup
  patchsetup
end
to computermove
  ask turtles [if lmao = 2 [wiggle
      ]]
end
to wiggle
  fd random foward-length
  rt random 180
  lt random 180
end
to gainpoints
ask turtles [ ifelse pcolor = black
 [set pcolor black set size size + 0]
 [ifelse pcolor = red
 [set size size / 2
   set pcolor black
   set points points / 2]
 [ set size size + .25
     set pcolor black
   set points points + 1
 ]]]
end
to start
  gainpoints
  computermove
  wait .1
enditall
enditall2
end
to mleft
  if lmao = 1 [set heading -90
  fd 1]
end
to mright
   if lmao = 1 [set heading 90
  fd 1]
end
to mup
   if lmao = 1 [set heading 0
  fd 1]
end
to mdown
   if lmao = 1 [set heading 180
fd 1
]
end
to enditall
  ask turtles [if count turtles in-radius 2 > 1 [die]]
end
to enditall2
  if count turtles = 1 [if count mains = 1 [ mainvictory]
    if count computers = 1 [computervictory]]
end
to mainvictory
  ca
  create-mains 1
  ask turtles [set size 10
    setxy 0 0
  set plabel "You Win!"]
end
to computervictory
  ca
  create-computers 1
  ask turtles [set size 10
    setxy 0 0
  set plabel  "Computer Wins"]
end
